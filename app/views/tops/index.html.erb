<div class="topPage">
  <div class="listWrapper">
    <% @lists.each do |list| %>
      <div class="list">
        <div class="list_header">
          <h2 class="list_header_title"><%= list.title %></h2>
          <div class="list_header_action">
            <%= link_to "削除", list_path(list.id), method: :delete %>
            <%= link_to "編集", edit_list_path(list.id) %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>